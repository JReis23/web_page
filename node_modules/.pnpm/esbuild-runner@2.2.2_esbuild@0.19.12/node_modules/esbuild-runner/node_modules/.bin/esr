#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/esbuild-runner@2.2.2_esbuild@0.19.12/node_modules/esbuild-runner/bin/node_modules:/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/esbuild-runner@2.2.2_esbuild@0.19.12/node_modules/esbuild-runner/node_modules:/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/esbuild-runner@2.2.2_esbuild@0.19.12/node_modules:/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/esbuild-runner@2.2.2_esbuild@0.19.12/node_modules/esbuild-runner/bin/node_modules:/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/esbuild-runner@2.2.2_esbuild@0.19.12/node_modules/esbuild-runner/node_modules:/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/esbuild-runner@2.2.2_esbuild@0.19.12/node_modules:/home/jr/code/Portfolio2024_ts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/esr.js" "$@"
else
  exec node  "$basedir/../../bin/esr.js" "$@"
fi
